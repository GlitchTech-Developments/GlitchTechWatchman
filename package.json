{
  "name": "gt-watcher",
  "version": "1.0.0",
  "description": "API testing tool made by GlitchTech Developments",
  "keywords": [
    "Javascript",
    "API Tester"
  ],
  "author": "GlitchTech Developments",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port=3000 --host ",
    "optimize": "vite optimize",
    "lint": "eslint ./src/app.ts",
    "format": "prettier --write ./src/**",
    "build": "tsc && vite build --minify --outDir ./out",
    "start": "vite preview --port=3000 --host"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "axios": "^1.2.2",
    "bootstrap": "^5.2.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  }
}
